{
    "name": "styde/form",
    "description": "Advanced form generation for Laravel",
    "type": "library",
    "require": {
        "php": "^7.3",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "6.*",
        "friendsofphp/php-cs-fixer": "^2.16",
        "styde/whetstone": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "Styde\\Form\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Duilio Palacios",
            "email": "duilio@styde.net"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": ["Styde\\Form\\FormServiceProvider"]
        }
    }
}
